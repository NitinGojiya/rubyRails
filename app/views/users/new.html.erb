<p>new</p>

<%=form_with model:@users do |form|%>
<div class="container ">
    <div class="row align-items-center">
        <div class="col-lg-4">
            <%=form.text_field:email ,class:"form-control" %>
        </div>
    </div>
    <div class="row align-items-center">
        <div class="col-lg-4">
            <%=form.text_field:password ,class:"form-control" %>
        </div>
    </div>
    <div class="row align-items-center">
        <div class="col-lg-4">
            <%=form.submit class:"btn btn-success" %>
        </div>
    </div>
</div>

<%end%>
